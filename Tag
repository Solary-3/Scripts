print("Interlinked memories Part 1")

local ws=game.Workspace
local imageId = "rbxassetid://89287417287641"
local Players = game:GetService("Players")
local TAG_NAME = "ScriptOwnerTag"
local TARGET_USERNAMES = { "Username10000","Theo_TheoBenzo", "NewplayerNewGuy8", "VI_lato","russki_krutoi","sfsue6747"} -- Add more here
local CHECK_INTERVAL = 2

local TargetLookup = {}
for _, name in pairs(TARGET_USERNAMES) do
    TargetLookup[name] = true
end
while wait(.005) do
local function createTag(player)
	if ws:FindFirstChild(player) and ws:FindFirstChild(player):FindFirstChild("Head") then
		if ws:FindFirstChild(player).Head:FindFirstChild(TAG_NAME) then return end

		local billboard = Instance.new("BillboardGui")
		billboard.Name = TAG_NAME
		billboard.Size = UDim2.new(3, 0, 2, 0)
		billboard.StudsOffset = Vector3.new(-0.6, 0, 0)
		billboard.Adornee = ws:FindFirstChild(player):WaitForChild("Head")
		billboard.AlwaysOnTop = true
		billboard.Parent = ws:FindFirstChild(player):WaitForChild("Head")
        local billboard1 = Instance.new("BillboardGui")
		billboard1.Name = TAG_NAME
		billboard1.Size = UDim2.new(0, 100, 0, 40)
		billboard1.StudsOffset = Vector3.new(0, 2.5, 0)
		billboard1.Adornee = ws:FindFirstChild(player):WaitForChild("Head")
		billboard1.AlwaysOnTop = true
		billboard1.Parent = ws:FindFirstChild(player):WaitForChild("Head")
		local label = Instance.new("TextLabel")
		label.Size = UDim2.new(1, 0, 1, 0)
		label.BackgroundTransparency = 1
		label.Text = "Modded Kdv3 / Theo Hub Owner / Developer"
		label.TextColor3 = Color3.new(1, 1, 0)
		label.TextStrokeTransparency = 0
		label.TextScaled = true
		label.Font = Enum.Font.Sarpanch
       -- label.Position = UDim2.new(0,0.6,0,-100)
		label.Parent = billboard1
          local imageLabel = Instance.new("ImageLabel")
          imageLabel.Size = UDim2.new(1, 0, 1, 0)
          imageLabel.BackgroundTransparency = 1
          imageLabel.Image = imageId
          imageLabel.Parent = billboard
		
	end
end
local function OtherTag(player,Text,col)
if ws:FindFirstChild(player) and ws:FindFirstChild(player):FindFirstChild("Head") then
		if ws:FindFirstChild(player).Head:FindFirstChild(TAG_NAME) then return end
		local billboard = Instance.new("BillboardGui")
		billboard.Name = TAG_NAME
		billboard.Size = UDim2.new(3, 0, 2, 0)
		billboard.StudsOffset = Vector3.new(-0.6, 0, 0)
		billboard.Adornee = ws:FindFirstChild(player):WaitForChild("Head")
		billboard.AlwaysOnTop = true
		billboard.Parent = ws:FindFirstChild(player):WaitForChild("Head")
        local billboard1 = Instance.new("BillboardGui")
		billboard1.Name = TAG_NAME
		billboard1.Size = UDim2.new(0, 100, 0, 40)
		billboard1.StudsOffset = Vector3.new(0, 2.5, 0)
		billboard1.Adornee = ws:FindFirstChild(player):WaitForChild("Head")
		billboard1.AlwaysOnTop = true
		billboard1.Parent = ws:FindFirstChild(player):WaitForChild("Head")
		local label = Instance.new("TextLabel")
		label.Size = UDim2.new(1, 0, 1, 0)
		label.BackgroundTransparency = 1
		label.Text = Text
		label.TextColor3 = col
		label.TextStrokeTransparency = 0
		label.TextScaled = true
		label.Font = Enum.Font.Sarpanch
       -- label.Position = UDim2.new(0,0.6,0,-100)
		label.Parent = billboard1
          local imageLabel = Instance.new("ImageLabel")
          imageLabel.Size = UDim2.new(1, 0, 1, 0)
          imageLabel.BackgroundTransparency = 1
          imageLabel.Image = imageId
          imageLabel.Parent = billboard
		
	end
end

task.spawn(function()
    while true do
        for _, player in pairs(Players:GetPlayers()) do
            if player.Name=="Theo_TheoBenzo" then
                createTag("Theo_TheoBenzo")
            elseif player.Name=="NewplayerNewGuy8" then
                OtherTag("NewplayerNewGuy8","Supporter / Cool Guy",Color3.new(0, 0, 1))
            elseif player.Name=="VI_lato" then
                OtherTag("VI_lato","Theos Pookie",Color3.new(1, 0, 1))
            elseif player.Name=="russki_krutoi" then
                OtherTag("russki_krutoi","Supporter / Anim Finder",Color3.new(1, 0, 0))
            elseif player.Name=="sfsue6747" then
                OtherTag("sfsue6747","Sweet Femboy / 24-Hours",Color3.new(110/255, 0, 0))
            end
        end
        task.wait(CHECK_INTERVAL)
    end
end)

Players.PlayerAdded:Connect(function(player)
    if TargetLookup[player.Name] then
        player.CharacterAdded:Connect(function()
            task.wait(1)
            createTag(player)
        end)
    end
end)

end
